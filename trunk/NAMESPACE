 summary.GeneralMultiChainLadder, print.GeneralMultiChainLadder, transf.Bcov,extract.cols )
export(MultiChainLadder, fit.MultiChainLadder, predict.MultiChainLadderFit, mse.MultiChainLadder)
importFrom(stats, quantile)

#Methods
S3method(plot, MackChainLadder)
S3method(plot, MunichChainLadder)
S3method(plot, BootChainLadder)
